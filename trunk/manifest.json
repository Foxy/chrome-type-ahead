{
  "name": "Type-ahead-find",
  "description": "Find as you type",
  "version": "0.1.1",
  "background_page": "background.html",
  "options_page": "options.html",
   "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "ftp://*/*", "file://*/*"],
      "js": ["type-ahead.js"]
    }
  ],
  "icons": {
    "48": "icon-48.png"
  }
}
