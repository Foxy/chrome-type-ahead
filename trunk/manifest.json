{
  "name": "Type-ahead-find",
  "description": "Search for links when you start typing",
  "version": "0.0.3",
  "background_page": "background.html",
  "options_page": "options.html",
   "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "file://*/*", "ftp://*/*"],
      "js": ["type-ahead.js"]
    }
  ],
  "icons": {
    "48": "icon-48.png"
  }
}
